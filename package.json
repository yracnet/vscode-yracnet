{
    "name": "vscode-yracnet-plugin",
    "displayName": "vscode-yracnet-plugin",
    "description": "Yracnet Source Template",
    "version": "0.0.2",
    "publisher": "yracnet",
    "author": {
        "name": "Willyams Yujra",
        "email": "yracnet@gmail.com"
    },
    "homepage": "https://github.com/yracnet/vscode-yracnet-plugin",
    "repository": {
        "type": "git",
        "url": "https://github.com/yracnet/vscode-yracnet-plugin"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
	"keywords": [
		"yracnet",
		"source",
		"code"
	],
    "contributes": {
        "languages": [
            {
                "id": "yracnet",
                "aliases": [
                    "Yracnet Source Code",
                    "yracnet"
                ],
                "extensions": [
                    ".yrc",
                    ".xml",
                    ".txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "yracnet",
                "scopeName": "source.yrc",
                "path": "./syntaxes/yracnet.tmLanguage.json",
                "embeddedLanguages": {}
            }
        ]
    }
}

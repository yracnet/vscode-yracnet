{
    "name": "vscode-yracnet",
    "displayName": "vscode-yracnet",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "yracnet",
